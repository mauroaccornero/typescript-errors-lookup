{
  "name": "typescript-errors-lookup",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run data:get && npm run data:refine && npm run build:dev && npm run serve",
    "build:prod": "cross-env NODE_ENV=production BASE_URL=https://typescript-errors-lookup.s3.eu-central-1.amazonaws.com ts-node ./scripts/build/build.ts",
    "build:dev": "cross-env NODE_ENV=development BASE_URL=http://localhost:4444 ts-node ./scripts/build/build.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "ts-node ./scripts/dev/server.ts",
    "data:get": "ts-node scripts/data/getData.ts",
    "data:refine": "ts-node scripts/data/refineData.ts",
    "ts:check": "tsc --noEmit",
    "audit": "npm audit --production",
    "precommit": "npm run lint: fix && npm run ts:check && npm run audit"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/html-minifier": "^4.0.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/rimraf": "^3.0.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-minifier": "^4.0.0",
    "pino": "^8.7.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0"
  },
  "description": "Unofficial and friendly typescript's errors lookup",
  "author": "Mauro Accornero <info@mauroaccornero.it>",
  "bugs": {
    "url" : "https://github.com/mauroaccornero/typescript-errors-lookup/issues",
    "email" : "info@mauroaccornero.it"
  },
  "repository": "github:mauroaccornero/typescript-errors-lookup",
  "license": "MIT"
}
